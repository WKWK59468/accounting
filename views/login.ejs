<%- include('./partial/header.ejs', {title: title, navBar: navBar}); %>
<div class="container mt-5 py-2 pl-5 p-sm-5">
  <div class="row my-5">
      <div class="col page_title">登入 Login</div>
  </div>
  <div class="row">
    <div class="col-12 col-md-6">
      <form>
        <div class="mb-5">
          <label for="email" class="form-label">Email</label>
          <input type="email" class="form-control" id="email" aria-describedby="emailHelp" placeholder="請輸入Email">
        </div>
        <div class="mb-5">
          <label for="password" class="form-label">Password</label>
          <input type="password" class="form-control" id="password" placeholder="請輸入密碼">
        </div>
        <button type="submit" class="login_btn2 btn btn-primary mt-4 mb-5 mb-sm-0">Login</button>
      </form>
    </div>
    <div class="col">
        <img id="notes" src="/images/notes.png" alt="notes">
    </div>
  </div>
</div>  
<%- include('./partial/footer.ejs'); %>